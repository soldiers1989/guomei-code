-- 内管系统操作日志表
CREATE TABLE CCS.CCS_OP_OPERATE_LOG
(
	ORG CHAR(12) NOT NULL,
	-- 操作序列号
	OP_SEQ NUMBER(16) NOT NULL,
	OP_ID VARCHAR2(40) NOT NULL,
	OP_TIME TIMESTAMP NOT NULL,
	BRANCH_ID VARCHAR2(9) NOT NULL,
	SERVICE_CODE CHAR(4) NOT NULL,
	CARD_NBR VARCHAR2(19),
	-- 顺序号，如未来需要校验位，可在后面补充
	CUST_ID NUMBER(16),
	RELATED_KEY VARCHAR2(50),
	RELATED_DESC VARCHAR2(100),
	JPA_VERSION NUMBER NOT NULL,
	PRIMARY KEY (OP_SEQ)
);


CREATE TABLE CCS.CCS_OP_PRIVILEGE
(
	ORG CHAR(12) NOT NULL,
	OP_ID VARCHAR2(40) NOT NULL,
	MAX_ACCT_TXN_ADJ NUMBER(15,2),
	MAX_POINTS_ADJ NUMBER(13),
	MAX_LMT_ADJ NUMBER(13),
	MAX_CASHLOAN_ADJ NUMBER(13),
	MAX_LOAN_APPROVE_ADJ NUMBER(13),
	MAX_POSLOAN_ADJ NUMBER(13) DEFAULT 0.00,
	OP_UPDATE_ID VARCHAR2(40) NOT NULL,
	OP_TIME TIMESTAMP NOT NULL,
	JPA_VERSION NUMBER NOT NULL,
	PRIMARY KEY (ORG, OP_ID)
);


CREATE TABLE CCS.CCS_PLAN
(
	ORG CHAR(12) NOT NULL,
	PLAN_ID NUMBER(16) NOT NULL,
	ACCT_NBR NUMBER(16) NOT NULL,
	-- ///
	-- @com.sunline.ppy.dictionary.enums.AccountType
	ACCT_TYPE CHAR(1) NOT NULL,
	-- 主卡逻辑卡号
	LOGIC_CARD_NBR VARCHAR2(19) NOT NULL,
	-- 来自信用计划模板中的PLAN_ID，用来表示信用计划类型
	PLAN_NBR CHAR(6) NOT NULL,
	-- ///
	-- @com.sunline.ppy.dictionary.enums.PlanType
	PLAN_TYPE CHAR(1) NOT NULL,
	PRODUCT_CD CHAR(6) NOT NULL,
	REF_NBR VARCHAR2(23),
	-- 当前欠款（负值表示有溢缴款）
	CURR_BAL NUMBER(15,2) NOT NULL,
	BEG_BAL NUMBER(15,2) NOT NULL,
	-- 当前争议金额
	DISPUTE_AMT NUMBER(15,2) NOT NULL,
	TOT_DUE_AMT NUMBER(15,2) NOT NULL,
	PLAN_ADD_DATE DATE NOT NULL,
	PAID_OUT_DATE DATE,
	PAST_PRINCIPAL NUMBER(15,2) NOT NULL,
	PAST_INTEREST NUMBER(15,2) NOT NULL,
	PAST_CARD_FEE NUMBER(15,2) NOT NULL,
	PAST_OVRLMT_FEE NUMBER(15,2) NOT NULL,
	PAST_LATE_FEE NUMBER(15,2) NOT NULL,
	-- 已出账单资金不足罚金（NONE SUFFICIENT FEE）
	PAST_NSFUND_FEE NUMBER(15,2) NOT NULL,
	-- 已出账单交易费
	PAST_TXN_FEE NUMBER(15,2) NOT NULL,
	-- 已出账单服务费
	PAST_SVC_FEE NUMBER(15,2) NOT NULL,
	-- 已出账单保险金额
	PAST_INSURANCE NUMBER(15,2) NOT NULL,
	-- 已出账单自定义费用1
	PAST_USER_FEE1 NUMBER(15,2) NOT NULL,
	-- 已出账单自定义费用2
	PAST_USER_FEE2 NUMBER(15,2) NOT NULL,
	-- 已出账单自定义费用3
	PAST_USER_FEE3 NUMBER(15,2) NOT NULL,
	-- 已出账单自定义费用4
	PAST_USER_FEE4 NUMBER(15,2) NOT NULL,
	-- 已出账单自定义费用5
	PAST_USER_FEE5 NUMBER(15,2) NOT NULL,
	-- 已出账单自定义费用6
	PAST_USER_FEE6 NUMBER(15,2) NOT NULL,
	-- 本账期本金
	CTD_PRINCIPAL NUMBER(15,2) NOT NULL,
	CTD_INTEREST NUMBER(15,2) NOT NULL,
	CTD_CARD_FEE NUMBER(15,2) NOT NULL,
	CTD_OVRLMT_FEE NUMBER(15,2) NOT NULL,
	CTD_LATE_FEE NUMBER(15,2) NOT NULL,
	-- 本账期资金不足罚金
	CTD_NSFUND_FEE NUMBER(15,2) NOT NULL,
	-- 本账期交易费
	CTD_SVC_FEE NUMBER(15,2) NOT NULL,
	-- 本账期服务费
	CTD_TXN_FEE NUMBER(15,2) NOT NULL,
	-- 本账期保险金额
	CTD_INSURANCE NUMBER(15,2) NOT NULL,
	CTD_USER_FEE1 NUMBER(15,2) NOT NULL,
	-- 本账期新增自定义费用2
	CTD_USER_FEE2 NUMBER(15,2) NOT NULL,
	-- 本账期新增自定义费用3
	CTD_USER_FEE3 NUMBER(15,2) NOT NULL,
	-- 本账期新增自定义费4
	CTD_USER_FEE4 NUMBER(15,2) NOT NULL,
	-- 本账期新增自定义费用5
	CTD_USER_FEE5 NUMBER(15,2) NOT NULL,
	-- 本账期新增自定义费6
	CTD_USER_FEE6 NUMBER(15,2) NOT NULL,
	-- 当期借记金额，统计当期新增的借记交易金额
	CTD_AMT_DB NUMBER(15,2) NOT NULL,
	-- 当期贷记金额
	CTD_AMT_CR NUMBER(15,2) NOT NULL,
	-- 当期借记交易笔数
	CTD_NBR_DB NUMBER NOT NULL,
	-- 当期贷记交易笔数
	CTD_NBR_CR NUMBER NOT NULL,
	-- 不享受免息期的交易已产生的未入账利息金额
	NODEFBNP_INT_ACRU NUMBER(15,4) NOT NULL,
	-- 期初余额利息累计
	BEG_DEFBNP_INT_ACRU NUMBER(15,4) NOT NULL,
	-- 当期余额累计的利息
	CTD_DEFBNP_INT_ACRU NUMBER(15,4) NOT NULL,
	USER_CODE1 VARCHAR2(4),
	USER_CODE2 VARCHAR2(4),
	USER_CODE3 VARCHAR2(4),
	USER_CODE4 VARCHAR2(4),
	USER_CODE5 VARCHAR2(4),
	USER_CODE6 VARCHAR2(4),
	USER_NUMBER1 NUMBER NOT NULL,
	USER_NUMBER2 NUMBER NOT NULL,
	USER_NUMBER3 NUMBER NOT NULL,
	USER_NUMBER4 NUMBER NOT NULL,
	USER_NUMBER5 NUMBER NOT NULL,
	USER_NUMBER6 NUMBER NOT NULL,
	USER_FIELD1 VARCHAR2(40),
	USER_FIELD2 VARCHAR2(40),
	USER_FIELD3 VARCHAR2(40),
	USER_FIELD4 VARCHAR2(40),
	USER_FIELD5 VARCHAR2(40),
	USER_FIELD6 VARCHAR2(40),
	USER_DATE1 DATE,
	USER_DATE2 DATE,
	USER_DATE3 DATE,
	USER_DATE4 DATE,
	USER_DATE5 DATE,
	USER_DATE6 DATE,
	USER_AMT1 NUMBER(15,2) NOT NULL,
	USER_AMT2 NUMBER(15,2) NOT NULL,
	USER_AMT3 NUMBER(15,2) NOT NULL,
	USER_AMT4 NUMBER(15,2) NOT NULL,
	USER_AMT5 NUMBER(15,2) NOT NULL,
	USER_AMT6 NUMBER(15,2) NOT NULL,
	JPA_VERSION NUMBER NOT NULL,
	PAST_PENALTY NUMBER(15,2) NOT NULL,
	CTD_PENALTY NUMBER(15,2) NOT NULL,
	PAST_COMPOUND NUMBER(15,2) NOT NULL,
	CTD_COMPOUND NUMBER(15,2) NOT NULL,
	PENALTY_ACRU NUMBER(15,4) NOT NULL,
	COMPOUND_ACRU NUMBER(15,4) NOT NULL,
	INTEREST_RATE NUMBER(12,4) NOT NULL,
	PENALTY_RATE NUMBER(12,4) NOT NULL,
	COMPOUND_RATE NUMBER(12,4) NOT NULL,
	-- ///
	-- @com.sunline.ppy.dictionary.enums.Indicator
	USE_PLAN_RATE CHAR(1) NOT NULL,
	LAST_PMT_DATE DATE,
	TERM NUMBER,
	CTD_STAMPDUTY_AMT NUMBER(15,2),
	PAST_STAMPDUTY_AMT NUMBER(15,2),
	CTD_ADDEDVALUETAX_AMT NUMBER(15,2),
	PAST_ADDEDVALUETAX_AMT NUMBER(15,2),
	CTD_MULCT_AMT NUMBER(15,2),
	PAST_MULCT_AMT NUMBER(15,2),
	PRIMARY KEY (PLAN_ID)
);


CREATE TABLE CCS.CCS_POINTS_ADJ_LOG
(
	ORG CHAR(12) NOT NULL,
