	-- 本账期消费笔数
	CTD_RETAIL_CNT NUMBER NOT NULL,
	CTD_CASH_AMT NUMBER(15,2) NOT NULL,
	-- 本账期取现笔数
	CTD_CASH_CNT NUMBER NOT NULL,
	MTD_RETAIL_AMT NUMBER(15,2) NOT NULL,
	-- 本月消费笔数
	MTD_RETAIL_CNT NUMBER NOT NULL,
	MTD_CASH_AMT NUMBER(15,2) NOT NULL,
	-- 本月取现笔数
	MTD_CASH_CNT NUMBER NOT NULL,
	YTD_RETAIL_AMT NUMBER(15,2) NOT NULL,
	-- 本年消费笔数
	YTD_RETAIL_CNT NUMBER NOT NULL,
	-- 本年取现金额
	YTD_CASH_AMT NUMBER(15,2) NOT NULL,
	-- 本年取现次数
	YTD_CASH_CNT NUMBER NOT NULL,
	LTD_RETAIL_AMT NUMBER(15,2) NOT NULL,
	-- 历史消费次数
	LTD_RETAIL_CNT NUMBER NOT NULL,
	LTD_CASH_AMT NUMBER(15,2) NOT NULL,
	LTD_CASH_CNT NUMBER NOT NULL,
	LAST_CYCLE_RETAIL_AMT NUMBER(15,2) NOT NULL,
	-- 上期消费笔数
	LAST_CYCLE_RETAIL_CNT NUMBER NOT NULL,
	LAST_CYCLE_CASH_AMT NUMBER(15,2) NOT NULL,
	LAST_CYCLE_CASH_CNT NUMBER NOT NULL,
	LAST_MTH_RETL_AMT NUMBER(15,2) NOT NULL,
	LAST_MTH_RETL_CNT NUMBER NOT NULL,
	LAST_MTH_CASH_AMT NUMBER(15,2) NOT NULL,
	LAST_MTH_CASH_CNT NUMBER NOT NULL,
	LAST_YEAR_RETL_AMT NUMBER(15,2) NOT NULL,
	LAST_YEAR_RETL_CNT NUMBER NOT NULL,
	LAST_YEAR_CASH_AMT NUMBER(15,2) NOT NULL,
	LAST_YEAR_CASH_CNT NUMBER NOT NULL,
	JPA_VERSION NUMBER NOT NULL,
	PRIMARY KEY (LOGIC_CARD_NBR)
);


