CREATE TABLE CCS.CCS_STATEMENT
(
	ORG CHAR(12) NOT NULL,
	ACCT_NBR NUMBER(16) NOT NULL,
	-- ///
	-- @com.sunline.ppy.dictionary.enums.AccountType
	-- 
	ACCT_TYPE CHAR(1) NOT NULL,
	STMT_DATE DATE NOT NULL,
	-- 客户姓名 - 冗余字段，从客户表复制过来，这样批量处理时可以不用关联客户信息
	NAME VARCHAR2(80) NOT NULL,
	PMT_DUE_DATE DATE,
	CURRENCY CHAR(3) NOT NULL,
	-- 标识账户是否出账单，及出联机账单还是实体账单
	-- Y|要求出账单
	-- N|不输出账单
	-- ///
	-- @com.sunline.ppy.dictionary.enums.Indicator
	STMT_FLAG CHAR(1) NOT NULL,
	CREDIT_LMT NUMBER(13) NOT NULL,
	TEMP_LMT NUMBER(13) NOT NULL,
	TEMP_LMT_BEG_DATE DATE,
	TEMP_LMT_END_DATE DATE,
	-- 上个账单日
	LAST_STMT_DATE DATE,
	STMT_BEG_BAL NUMBER(15,2) NOT NULL,
	STMT_CURR_BAL NUMBER(15,2) NOT NULL,
	-- 持卡人还清该欠款，则会对账户进行全额还款免息，该金额会显示在账单上，小于等于账户账单余额
	QUAL_GRACE_BAL NUMBER(15,2) NOT NULL,
	TOT_DUE_AMT NUMBER(15,2) NOT NULL,
	CTD_CASH_AMT NUMBER(15,2) NOT NULL,
	-- 本账期取现笔数
	CTD_CASH_CNT NUMBER NOT NULL,
	-- 本账期联机借记金额累计值，在授权时维护，批量不参与
	CTD_RETAIL_AMT NUMBER(15,2) NOT NULL,
	-- 本账期消费笔数
	CTD_RETAIL_CNT NUMBER NOT NULL,
	-- 本账期还款金额
	CTD_REPAY_AMT NUMBER(15,2) NOT NULL,
	-- 本账期还款笔数
	CTD_REPAY_CNT NUMBER NOT NULL,
	-- 本账期借记调整金额
	CTD_DB_ADJ_AMT NUMBER(15,2) NOT NULL,
	-- 本账期借记调整笔数
	CTD_DB_ADJ_CNT NUMBER NOT NULL,
	-- 本账期贷记调整金额
	CTD_CR_ADJ_AMT NUMBER(15,2) NOT NULL,
	-- 本账期贷记调整笔数
	CTD_CR_ADJ_CNT NUMBER NOT NULL,
	-- 本账期费用金额
	CTD_FEE_AMT NUMBER(15,2) NOT NULL,
	-- 本账期费用笔数
	CTD_FEE_CNT NUMBER NOT NULL,
	CTD_INTEREST_AMT NUMBER(15,2),
	CTD_INTEREST_CNT NUMBER,
	-- 本账期退货金额
	CTD_REFUND_AMT NUMBER(15,2) NOT NULL,
	-- 本账期退货笔数
	CTD_REFUND_CNT NUMBER NOT NULL,
	-- 当期借记金额，统计当期新增的借记交易金额
	CTD_AMT_DB NUMBER(15,2) NOT NULL,
	-- 当期借记交易笔数
	CTD_NBR_DB NUMBER NOT NULL,
	-- 当期贷记金额
	CTD_AMT_CR NUMBER(15,2) NOT NULL,
	-- 当期贷记交易笔数
	CTD_NBR_CR NUMBER NOT NULL,
	-- 实体账单不需要显示此域（但可以用作账单拖欠信息提醒）， 联机账单需要
	AGE_CODE CHAR(1) NOT NULL,
	-- ///
	-- @com.sunline.ppy.dictionary.enums.Indicator
	-- 
	GRACE_DAYS_FULL_IND CHAR(1) NOT NULL,
	-- 超限日期
	OVRLMT_DATE DATE,
	POINTS_BEG_BAL NUMBER(13) NOT NULL,
	-- 当期新增积分
	CTD_POINTS NUMBER(13) NOT NULL,
	-- 当期调整积分
	CTD_ADJ_POINTS NUMBER(13) NOT NULL,
	-- 当期兑换积分
	CTD_SPEND_POINTS NUMBER(13) NOT NULL,
	-- 积分余额
	POINTS_BAL NUMBER(13) NOT NULL,
	BLOCK_CODE VARCHAR2(27),
	-- 取值为M,F时账单系统需要将本外币账单接口合并生成实体账单
	-- N|不还款
	-- M|最小额还款
	-- F|全额还款
	-- ///
	-- @com.sunline.ppy.dictionary.enums.DualBillingInd
	DUAL_BILLING_FLAG CHAR(1) NOT NULL,
	-- 取现额度比例（占用账户信用额度）
	CASH_LMT_RATE NUMBER(5,2),
	-- 额度内分期额度比例
	LOAN_LMT_RATE NUMBER(5,2),
	CURR_DUE_AMT NUMBER(15,2) NOT NULL,
	-- 上个月最小还款额
	PAST_DUE_AMT1 NUMBER(15,2) NOT NULL,
	-- 上2个月最小还款额
	PAST_DUE_AMT2 NUMBER(15,2) NOT NULL,
	-- 上3个月最小还款额
	PAST_DUE_AMT3 NUMBER(15,2) NOT NULL,
	-- 上4个月最小还款额
	PAST_DUE_AMT4 NUMBER(15,2) NOT NULL,
	-- 上5个月最小还款额
	PAST_DUE_AMT5 NUMBER(15,2) NOT NULL,
	-- 上6个月最小还款额
	PAST_DUE_AMT6 NUMBER(15,2) NOT NULL,
	-- 上7个月最小还款额
	PAST_DUE_AMT7 NUMBER(15,2) NOT NULL,
	-- 上8个月最小还款额
	PAST_DUE_AMT8 NUMBER(15,2) NOT NULL,
	JPA_VERSION NUMBER NOT NULL,
	-- 取自默认卡产品的主卡卡号，用于作为滞纳金、超限费的入账卡号
	DEFAULT_LOGIC_CARD_NBR VARCHAR2(19) NOT NULL,
	EMAIL VARCHAR2(80),
	-- M|男
	-- F|女
	-- ///
	-- @com.sunline.ppy.dictionary.enums.Gender
	GENDER CHAR(1),
	MOBILE_NO VARCHAR2(20),
	-- 账单地址
	STMT_ADDRESS VARCHAR2(200),
	-- 账单地址城市
	STMT_CITY VARCHAR2(40),
	-- 账单地址国家代码（从客户信息复制过来，保证核心快速处理）
	STMT_COUNTRY_CODE CHAR(3),
	-- 账单地址行政区
	STMT_DISTRICT VARCHAR2(40),
	-- P|纸质账单
	-- E|电子账单
	-- M|彩信账单
	-- B|both
	-- ///
	-- @com.sunline.ppy.dictionary.enums.StmtMediaType
	STMT_MEDIA_TYPE CHAR(1) NOT NULL,
	-- 账单地址省份
	STMT_STATE VARCHAR2(40),
	-- 账单地址邮政编码
	STMT_POSTCODE VARCHAR2(10),
	LAST_STMT_BAL NUMBER(15,2) NOT NULL,
	CTD_STMT_BAL NUMBER(15,2) NOT NULL,
	PRIMARY KEY (ACCT_NBR, ACCT_TYPE, STMT_DATE)
);


-- 交易流水历史 - 在入账的同时写该表
CREATE TABLE CCS.CCS_STMT_REPRINT_REG
(
	ORG CHAR(12) NOT NULL,
	-- 如果一个账户需要补打多期账单，则需要生成多笔补打申请流水，每笔流水分配一个流水号
	REPRINT_SEQ NUMBER(16) NOT NULL,
	ACCT_NBR NUMBER(16) NOT NULL,
	STMT_DATE DATE NOT NULL,
	JPA_VERSION NUMBER NOT NULL,
	TXN_DATE DATE NOT NULL,
	-- 请求日期时间
	REQUEST_TIME TIMESTAMP NOT NULL,
	CARD_NBR VARCHAR2(19) NOT NULL,
	PRIMARY KEY (REPRINT_SEQ)
);


CREATE TABLE CCS.CCS_TXN_ADJ_LOG
(
	ORG CHAR(12) NOT NULL,
	-- 操作序列号
	OP_SEQ NUMBER(16) NOT NULL,
	OP_TIME TIMESTAMP NOT NULL,
	OP_ID VARCHAR2(40),
	ACCT_NBR NUMBER(16) NOT NULL,
	CARD_NBR VARCHAR2(19),
	TXN_CODE CHAR(4) NOT NULL,
	-- D|借记交易
	-- C|贷记交易
	-- M|memo交易
	-- ///
	-- @com.sunline.ppy.dictionary.enums.DbCrInd
	DB_CR_IND CHAR(1) NOT NULL,
	TXN_AMT NUMBER(15,2) NOT NULL,
	CURRENCY CHAR(3) NOT NULL,
	REF_NBR VARCHAR2(23),
	REMARK VARCHAR2(40) NOT NULL,
	JPA_VERSION NUMBER NOT NULL,
	AUTH_CODE CHAR(6) NOT NULL,
	MCC CHAR(4) NOT NULL,
	B003_PROC_CODE VARCHAR2(6) NOT NULL,
	B004_AMT NUMBER(15,2),
	B007_TXN_TIME VARCHAR2(10) NOT NULL,
	B011_TRACE CHAR(6) NOT NULL,
	B032_ACQ_INST VARCHAR2(15) NOT NULL,
	B033_FWD_INS VARCHAR2(40) NOT NULL,
	B049_CURR_CODE CHAR(3) NOT NULL,
	MTI CHAR(4) NOT NULL,
	-- /// 
	-- @com.sunline.ppy.dictionary.enums.Indicator
	VOID_IND CHAR(1) NOT NULL,
	VOID_TIME TIMESTAMP,
	VOID_REASON VARCHAR2(80),
	VOID_OP_ID VARCHAR2(8),
	B042_MER_ID VARCHAR2(15) NOT NULL,
	B039_RTN_CODE VARCHAR2(2),
	TXN_DATE DATE NOT NULL,
	LOG_BIZ_DATE DATE NOT NULL,
	TERM NUMBER,
	PRIMARY KEY (OP_SEQ)
);


-- 逻辑卡/介质对照表，只用于批量接口程序查介质卡号确定逻辑卡号。
-- 每天批量开始drop后重建。
CREATE TABLE CCS.CCS_TXN_GL
(
	ORG CHAR(12) NOT NULL,
	CURRENCY CHAR(3),
	TXN_SEQ NUMBER(16) NOT NULL,
	ACCT_NBR NUMBER(16) NOT NULL,
	-- ///
	-- @com.sunline.ppy.dictionary.enums.AccountType
	-- 
	ACCT_TYPE CHAR(1) NOT NULL,
	-- 实体账单不需要显示此域（但可以用作账单拖欠信息提醒）， 联机账单需要
	AGE_CODE CHAR(1) NOT NULL,
	TXN_CODE CHAR(4) NOT NULL,
	-- D|借记交易
	-- C|贷记交易
	-- M|memo交易
	-- ///
	-- @com.sunline.ppy.dictionary.enums.DbCrInd
	DB_CR_IND CHAR(1) NOT NULL,
	POST_DATE DATE NOT NULL,
	POST_AMT NUMBER(15,2) NOT NULL,
	-- N|normal 正常入账交易
	-- S|spend 挂账交易
	-- W|write off 核销交易
	-- ///
	-- @com.sunline.ppy.dictionary.enums.PostGlIndicator
	POST_GL_IND CHAR(1) NOT NULL,
	OWNING_BRANCH VARCHAR2(9) NOT NULL,
	JPA_VERSION NUMBER NOT NULL,
	PRIMARY KEY (TXN_SEQ)
);


-- 交易流水历史 - 在入账的同时写该表
CREATE TABLE CCS.CCS_TXN_HST
(
	ORG CHAR(12) NOT NULL,
	TXN_SEQ NUMBER(16) NOT NULL,
	ACCT_NBR NUMBER(16) NOT NULL,
	-- ///
	-- @com.sunline.ppy.dictionary.enums.AccountType
	-- 
	ACCT_TYPE CHAR(1) NOT NULL,
	CARD_NBR VARCHAR2(19) NOT NULL,
	-- 主卡逻辑卡号
	LOGIC_CARD_NBR VARCHAR2(19) NOT NULL,
	-- 逻辑卡主卡卡号
	CARD_BASIC_NBR VARCHAR2(19),
	PRODUCT_CD CHAR(6),
	TXN_DATE DATE NOT NULL,
	TXN_TIME TIMESTAMP NOT NULL,
	-- M|金融交易
	-- P|积分交易
	-- ///
	-- @com.sunline.ppy.dictionary.enums.PostTxnType
	POST_TXN_TYPE CHAR(1) NOT NULL,
	TXN_CODE CHAR(4) NOT NULL,
	-- D|借记交易
	-- C|贷记交易
	-- M|memo交易
	-- ///
	-- @com.sunline.ppy.dictionary.enums.DbCrInd
	DB_CR_IND CHAR(1),
	TXN_AMT NUMBER(15,2) NOT NULL,
	POST_AMT NUMBER(15,2) NOT NULL,
	POST_DATE DATE NOT NULL,
	AUTH_CODE CHAR(6),
	-- 卡片锁定码
	CARD_BLOCK_CODE VARCHAR2(27),
	TXN_CURRENCY CHAR(3) NOT NULL,
	-- 清算货币码
	POST_CURRENCY CHAR(3) NOT NULL,
	ORIG_TRANS_DATE DATE,
	-- 来自信用计划模板中的PLAN_ID，用来表示信用计划类型
	PLAN_NBR CHAR(6),
	REF_NBR VARCHAR2(23),
	TXN_DESC VARCHAR2(80) NOT NULL,
	-- 用于账单交易描述显示
	TXN_SHORT_DESC VARCHAR2(80),
	POINTS NUMBER(13) NOT NULL,
	-- ///
	-- @com.sunline.ppy.dictionary.enums.PostingFlag
	POSTING_FLAG CHAR(3) NOT NULL,
	-- 当天入账交易填F00
	-- ///
	-- @com.sunline.ppy.dictionary.enums.PostingFlag
	PRE_POSTING_FLAG CHAR(3),
	-- 公司卡还款金额
	REL_PMT_AMT NUMBER(15,2) NOT NULL,
	-- 还款交易原始金额
	ORIG_PMT_AMT NUMBER(15,2) NOT NULL,
	-- 受理分行代码
	ACQ_BRANCH_IQ CHAR(9),
	-- 受理机构终端标识码
	ACQ_TERMINAL_ID VARCHAR2(8),
	-- 受理机构标识码
	ACQ_ACCEPTOR_ID VARCHAR2(15),
	-- 受卡点（商户）名称，地址
	ACQ_ADDRESS VARCHAR2(40),
	MCC CHAR(4),
	-- 原交易交易码
	ORIG_TXN_CODE CHAR(4),
	-- 原交易清算金额
	ORIG_TXN_AMT NUMBER(15,2) NOT NULL,
	-- 原交易交易金额
	ORIG_SETT_AMT NUMBER(15,2) NOT NULL,
	-- 原交易货币转换费
	INTERCHANGE_FEE NUMBER(15,2) NOT NULL,
	-- 发卡方应付手续费
	FEE_PAYOUT NUMBER(15,2) NOT NULL,
	-- 发卡方应得手续费
	FEE_PROFIT NUMBER(15,2) NOT NULL,
	-- 分期交易发卡行收益
	LOAN_ISSUE_PROFIT NUMBER(15,2) NOT NULL,
	STMT_DATE DATE,
	-- 销售单凭证号-必须全部是数字
	VOUCHER_NO VARCHAR2(7),
	JPA_VERSION NUMBER NOT NULL,
	TERM NUMBER,
	PRIMARY KEY (TXN_SEQ)
);


