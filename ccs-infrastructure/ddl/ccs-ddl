CREATE TABLE CCS.CCS_LINKMAN
(
	LINKMAN_ID NUMBER(16) NOT NULL,
	-- 顺序号，如未来需要校验位，可在后面补充
	CUST_ID NUMBER(16) NOT NULL,
	-- C|配偶
	-- F|父亲
	-- M|母亲
	-- B|兄弟
	-- S|姐妹
	-- L|亲属
	-- W|同事
	-- ///
	-- @com.sunline.ppy.dictionary.enums.Relationship
	RELATIONSHIP CHAR(1) NOT NULL,
	ORG CHAR(12) NOT NULL,
	-- 客户姓名 - 冗余字段，从客户表复制过来，这样批量处理时可以不用关联客户信息
	NAME VARCHAR2(80) NOT NULL,
	-- M|男
	-- F|女
	-- ///
	-- @com.sunline.ppy.dictionary.enums.Gender
	GENDER CHAR(1),
	MOBILE_NO VARCHAR2(20) NOT NULL,
	BIRTHDAY DATE,
	CORP_NAME VARCHAR2(80),
	-- ///
	-- @com.sunline.ppy.dictionary.enums.IdType
	ID_TYPE VARCHAR2(1),
	ID_NO VARCHAR2(30),
	CORP_TELEPH_NBR VARCHAR2(20),
	CORP_FAX VARCHAR2(20),
	-- ///
	-- @com.sunline.ppy.dictionary.enums.EmpPositionAttrType
	CORP_POSITION CHAR(1),
	JPA_VERSION NUMBER NOT NULL,
	PRIMARY KEY (LINKMAN_ID),
	CONSTRAINT businessKey UNIQUE (CUST_ID, RELATIONSHIP)
);


CREATE TABLE CCS.CCS_LOAN
(
	ORG CHAR(12) NOT NULL,
	LOAN_ID NUMBER(16) NOT NULL,
	ACCT_NBR NUMBER(16) NOT NULL,
	-- ///
	-- @com.sunline.ppy.dictionary.enums.AccountType
	-- 
	ACCT_TYPE CHAR(1) NOT NULL,
	REF_NBR VARCHAR2(23) NOT NULL,
	DUE_BILL_NO VARCHAR2(30),
	-- 主卡逻辑卡号
	LOGIC_CARD_NBR VARCHAR2(19) NOT NULL,
	CARD_NBR VARCHAR2(19) NOT NULL,
	REGISTER_DATE DATE NOT NULL,
	-- 请求日期时间
	REQUEST_TIME TIMESTAMP NOT NULL,
	-- R|消费转分期
	-- C|现金转分期
	-- B|账单分期
	-- P|POS分期
	-- M|大额分期（专项分期）
	-- ///
	-- @com.sunline.ppy.dictionary.enums.LoanType
	LOAN_TYPE VARCHAR2(5) NOT NULL,
	-- ///
	-- @com.sunline.ppy.dictionary.enums.LoanStatus
	LOAN_STATUS CHAR(1) NOT NULL,
	-- I|注册但未活动
	-- A|活动状态(active)
	-- M|人工终止(manual terminate)
	-- D|逾期终止(delinquncy terminate)
	-- R|退货终止(refund)
	-- ///
	-- @com.sunline.ppy.dictionary.enums.LoanStatus
	-- 
	LAST_LOAN_STATUS CHAR(1),
	LOAN_INIT_TERM NUMBER NOT NULL,
	CURR_TERM NUMBER NOT NULL,
	REMAIN_TERM NUMBER NOT NULL,
	LOAN_INIT_PRIN NUMBER(15,2) NOT NULL,
	LOAN_FIXED_PMT_PRIN NUMBER(15,2) NOT NULL,
	LOAN_FIRST_TERM_PRIN NUMBER(15,2) NOT NULL,
	LOAN_FINAL_TERM_PRIN NUMBER(15,2) NOT NULL,
	LOAN_INIT_FEE NUMBER(15,2) NOT NULL,
	LOAN_FIXED_FEE NUMBER(15,2) NOT NULL,
	LOAN_FIRST_TERM_FEE NUMBER(15,2) NOT NULL,
	LOAN_FINAL_TERM_FEE NUMBER(15,2) NOT NULL,
	UNSTMT_PRIN NUMBER(15,2) NOT NULL,
	UNSTMT_FEE NUMBER(15,2) NOT NULL,
	ACTIVE_DATE DATE,
	PAID_OUT_DATE DATE,
	TERMINAL_DATE DATE,
	-- V|持卡人主动终止（volunteer）
	-- M|银行业务人员手工终止（manual）
	-- D|逾期自动终止（delinquncy）
	-- ///
	-- @com.sunline.ccs.param.def.enums.LoanTerminateReason
	TERMINAL_REASON_CD CHAR(1),
	PAID_PRINCIPAL NUMBER(15,2) NOT NULL,
	PAID_INTEREST NUMBER(15,2) NOT NULL,
	PAID_FEE NUMBER(15,2) NOT NULL,
	LOAN_CURR_BAL NUMBER(15,2) NOT NULL,
	LOAN_BAL_XFROUT NUMBER(15,2) NOT NULL,
	LOAN_BAL_XFRIN NUMBER(15,2) NOT NULL,
	LOAN_PRIN_XFROUT NUMBER(15,2) NOT NULL,
	LOAN_PRIN_XFRIN NUMBER(15,2) NOT NULL,
	LOAN_FEE_XFROUT NUMBER(15,2) NOT NULL,
	LOAN_FEE_XFRIN NUMBER(15,2) NOT NULL,
	ORIG_TXN_AMT NUMBER(15,2) NOT NULL,
	ORIG_TRANS_DATE DATE,
	ORIG_AUTH_CODE CHAR(6),
	JPA_VERSION NUMBER NOT NULL,
	LOAN_CODE VARCHAR2(4) NOT NULL,
	REGISTER_ID NUMBER(16) NOT NULL,
	EXTEND_INIT_PRIN NUMBER(15,2) NOT NULL,
	EXTEND_DATE DATE,
	BEF_EXTEND_FIXED_PMT_PRIN NUMBER(15,2) NOT NULL,
	BEF_EXTEND_INIT_TERM NUMBER,
	BEF_EXTEND_FIRST_TERM_PRIN NUMBER(15,2) NOT NULL,
	BEF_EXTEND_FINAL_TERM_PRIN NUMBER(15,2) NOT NULL,
	BEF_EXTEND_INIT_FEE NUMBER(15,2) NOT NULL,
	BEF_EXTEND_FIXED_FEE NUMBER(15,2) NOT NULL,
	BEF_EXTEND_FIRST_TERM_FEE NUMBER(15,2) NOT NULL,
	BEF_EXTEND_FINAL_TERM_FEE NUMBER(15,2) NOT NULL,
	EXTEND_FIRST_TERM_FEE NUMBER(15,2) NOT NULL,
	-- 联机时填入，一期固定为E
	-- F|一次性收取
	-- E|分期收取
	-- ///
	-- @com.sunline.ppy.dictionary.enums.LoanFeeMethod
	LOAN_FEE_METHOD CHAR(1) NOT NULL,
	INTEREST_RATE NUMBER(12,4),
	PENALTY_RATE NUMBER(12,4),
	COMPOUND_RATE NUMBER(12,4),
	FLOAT_RATE NUMBER(5,2),
	LOAN_EXPIRE_DATE DATE,
	-- 通过每个账单日扫描plan的还款状态，应还未还的转入plan个数
	LOAN_AGE_CODE VARCHAR2(2),
	-- ‎记录过去24个月的还款历史，每个字节的含义如下：
	-- 引用枚举值com.sunline.ppy.dictionary.enums.PaymentStatus
	-- B|全额还款
	-- D|部分还款
	-- U|还款未达最小还款额
	-- N|未还款
	-- C|无需还款
	PAYMENT_HST VARCHAR2(24),
	-- 本账期还款金额
	CTD_REPAY_AMT NUMBER(15,2),
	PAST_EXTEND_CNT NUMBER,
	PAST_SHORTEN_CNT NUMBER,
	ADV_PMT_AMT NUMBER(15,2),
	LAST_ACTION_DATE DATE,
	-- ///
	-- @com.sunline.ppy.dictionary.enums.LoanAction
	LAST_ACTION_TYPE CHAR(1),
	CONTR_NBR VARCHAR2(20),
	GUARANTY_ID VARCHAR2(32),
	STAMPDUTY_AMT NUMBER(15,2),
	UNSTMT_STAMPDUTY_AMT NUMBER(15,2),
	LOAN_STAMPDUTY_AMT NUMBER(15,2),
	PAID_STAMPDUTY_AMT NUMBER(15,2),
	ADDEDVALUETAX_AMT NUMBER(15,2),
	UNSTMT_ADDEDVALUETAX_AMT NUMBER(15,2),
	LOAN_ADDEDVALUETAX_AMT NUMBER(15,2),
	PAID_ADDEDVALUETAX_AMT NUMBER(15,2),
	ADDEDVALUETAX_RATE NUMBER(12,4),
	STAMPDUTY_RATE NUMBER(12,4),
	INSURANCE_RATE NUMBER(12,4),
	OVERDUE_DATE DATE,
	LAST_PENALTY_DATE DATE,
	-- 分期总保费
	INSURANCE_AMT NUMBER(15,2),
	UNSTMT_INSURANCE_AMT NUMBER(15,2),
	LOAN_INSURANCE_AMT NUMBER(15,2),
	PAID_INSURANCE_AMT NUMBER(15,2),
	-- 联机时填入，一期固定为E
	-- F|一次性收取
	-- E|分期收取
	-- ///
	-- @com.sunline.ppy.dictionary.enums.LoanFeeMethod
	LOAN_INS_FEE_METHOD CHAR(1),
	-- 联机时填入，一期固定为E
	-- F|一次性收取
	-- E|分期收取
	-- ///
	-- @com.sunline.ppy.dictionary.enums.LoanFeeMethod
	STAMPDUTY_METHOD CHAR(1),
	-- 联机时填入，一期固定为E
	-- F|一次性收取
	-- E|分期收取
	-- ///
	-- @com.sunline.ppy.dictionary.enums.LoanFeeMethod
	ADDEDVALUETAX_METHOD CHAR(1),
	PRIMARY KEY (LOAN_ID)
);


