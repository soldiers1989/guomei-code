);


CREATE TABLE CCS.CCS_ACCT
(
	ORG CHAR(12) NOT NULL,
	ACCT_NBR NUMBER(16) NOT NULL,
	-- ///
	-- @com.sunline.ppy.dictionary.enums.AccountType
	-- 
	ACCT_TYPE CHAR(1) NOT NULL,
	-- 顺序号，如未来需要校验位，可在后面补充
	CUST_ID NUMBER(16) NOT NULL,
	CUST_LMT_ID NUMBER(16) NOT NULL,
	PRODUCT_CD CHAR(6) NOT NULL,
	-- 取自默认卡产品的主卡卡号，用于作为滞纳金、超限费的入账卡号
	DEFAULT_LOGIC_CARD_NBR VARCHAR2(19) NOT NULL,
	CURRENCY CHAR(3) NOT NULL,
	CREDIT_LMT NUMBER(13) NOT NULL,
	TEMP_LMT NUMBER(13) NOT NULL,
	TEMP_LMT_BEG_DATE DATE,
	TEMP_LMT_END_DATE DATE,
	-- 取现额度比例（占用账户信用额度）
	CASH_LMT_RATE NUMBER(5,2),
	-- 授权允许的超限比例，来自账户下最高级别卡产品的参数定义。
	OVRLMT_RATE NUMBER(5,2),
	-- 额度内分期额度比例
	LOAN_LMT_RATE NUMBER(5,2),
	-- 当前欠款（负值表示有溢缴款）
	CURR_BAL NUMBER(15,2) NOT NULL,
	CASH_BAL NUMBER(15,2) NOT NULL,
	-- 当前本金余额
	PRINCIPAL_BAL NUMBER(15,2) NOT NULL,
	-- 当前额度内分期余额
	LOAN_BAL NUMBER(15,2) NOT NULL,
	-- 当前争议金额
	DISPUTE_AMT NUMBER(15,2) NOT NULL,
	BEG_BAL NUMBER(15,2) NOT NULL,
	-- 到期还款日余额
	PMT_DUE_DAY_BAL NUMBER(15,2) NOT NULL,
	-- 持卡人还清该欠款，则会对账户进行全额还款免息，该金额会显示在账单上，小于等于账户账单余额
	QUAL_GRACE_BAL NUMBER(15,2) NOT NULL,
	-- ///
	-- @com.sunline.ppy.dictionary.enums.Indicator
	-- 
	GRACE_DAYS_FULL_IND CHAR(1) NOT NULL,
	POINTS_BEG_BAL NUMBER(13) NOT NULL,
	-- 当期新增积分
	CTD_POINTS NUMBER(13) NOT NULL,
	-- 当期兑换积分
	CTD_SPEND_POINTS NUMBER(13) NOT NULL,
	-- 当期调整积分
	CTD_ADJ_POINTS NUMBER(13) NOT NULL,
	-- 积分余额
	POINTS_BAL NUMBER(13) NOT NULL,
	SETUP_DATE DATE NOT NULL,
	-- 账户睡眠日期
	DORMENT_DATE DATE,
	-- 账户恢复活动日期
	EACTIVE_DATE DATE,
	-- 超限日期
	OVRLMT_DATE DATE,
	-- 连续超限账期
	OVRLMT_NBR_OF_CYC NUMBER NOT NULL,
	-- 客户姓名 - 冗余字段，从客户表复制过来，这样批量处理时可以不用关联客户信息
	NAME VARCHAR2(80) NOT NULL,
	-- M|男
	-- F|女
	-- ///
	-- @com.sunline.ppy.dictionary.enums.Gender
	GENDER CHAR(1),
	OWNING_BRANCH VARCHAR2(9) NOT NULL,
	MOBILE_NO VARCHAR2(20),
	CORP_NAME VARCHAR2(80),
	-- 账户的账单日期
	CYCLE_DAY CHAR(2) NOT NULL,
	-- 标识账户是否出账单，及出联机账单还是实体账单
	-- Y|要求出账单
	-- N|不输出账单
	-- ///
	-- @com.sunline.ppy.dictionary.enums.Indicator
	STMT_FLAG CHAR(1) NOT NULL,
	-- ///
	-- @com.sunline.ppy.dictionary.enums.AddressType
	-- 
	STMT_MAIL_ADDR_IND CHAR(1),
	-- P|纸质账单
	-- E|电子账单
	-- M|彩信账单
	-- B|both
	-- ///
	-- @com.sunline.ppy.dictionary.enums.StmtMediaType
	STMT_MEDIA_TYPE CHAR(1) NOT NULL,
	-- 账单地址国家代码（从客户信息复制过来，保证核心快速处理）
	STMT_COUNTRY_CODE CHAR(3),
	-- 账单地址省份
	STMT_STATE VARCHAR2(40),
	-- 账单地址城市
	STMT_CITY VARCHAR2(40),
	-- 账单地址行政区
	STMT_DISTRICT VARCHAR2(40),
	-- 账单地址
	STMT_ADDRESS VARCHAR2(200),
	-- 账单地址邮政编码
	STMT_POSTCODE VARCHAR2(10),
	EMAIL VARCHAR2(80),
	BLOCK_CODE VARCHAR2(27),
	-- 实体账单不需要显示此域（但可以用作账单拖欠信息提醒）， 联机账单需要
	AGE_CODE CHAR(1) NOT NULL,
	AGE_CODE_GL CHAR(1) NOT NULL,
	MEMO_DB NUMBER(15,2) NOT NULL,
	MEMO_CASH NUMBER(15,2) NOT NULL,
	MEMO_CR NUMBER(15,2) NOT NULL,
	-- M|最小额扣款
	-- F|全额扣款
	-- C|最小额储蓄购汇
	-- E|全额储蓄购汇
	-- ///
	-- @com.sunline.ppy.dictionary.enums.DdIndicator
	DD_IND CHAR(1) NOT NULL,
	-- 约定还款扣款账户银行名称
	DD_BANK_NAME VARCHAR2(160),
	DD_BANK_BRANCH VARCHAR2(9),
	-- 约定还款扣款账号
	DD_BANK_ACCT_NBR VARCHAR2(40),
	-- 约定还款扣款账户姓名
	DD_BANK_ACCT_NAME VARCHAR2(160),
	-- 上期约定还款金额
	LAST_DD_AMT NUMBER(15,2) NOT NULL,
	-- 上期约定还款日期
	LAST_DD_DATE DATE,
	-- 取值为M,F时账单系统需要将本外币账单接口合并生成实体账单
	-- N|不还款
	-- M|最小额还款
	-- F|全额还款
	-- ///
	-- @com.sunline.ppy.dictionary.enums.DualBillingInd
	DUAL_BILLING_FLAG CHAR(1) NOT NULL,
	-- 上笔还款金额
	LAST_PMT_AMT NUMBER(15,2) NOT NULL,
	LAST_PMT_DATE DATE,
	-- 上个账单日
	LAST_STMT_DATE DATE,
	-- 上个到期还款日
	LAST_PMT_DUE_DATE DATE,
	-- 上次账龄提升日，可以用来代替DELQ DAYS的计算
	LAST_AGING_DATE DATE,
	-- 进入催收日期
	COLLECT_IN_DATE DATE,
	-- 出催收队列时间
	COLLECT_OUT_DATE DATE,
	-- 下个账单日
	NEXT_STMT_DATE DATE NOT NULL,
	PMT_DUE_DATE DATE,
	-- 约定还款扣款文件生成日期
	DD_DATE DATE,
	-- 宽限日
	GRACE_DATE DATE,
	-- 本币还外币日期
	DUAL_BILLING_DATE DATE,
	-- 首个账单日
	FIRST_STMT_DATE DATE,
	-- 转呆账日期
	CHARGE_OFF_DATE DATE,
	-- 卡片账户置上待销卡销户锁定码的日期
	-- 在销卡时置上销卡日期，在销卡撤销时清零销卡日期
	CLOSE_DATE DATE,
	-- 最终销户日期，指账户closed
	CLOSED_DATE DATE,
	-- 首次消费日期，无消费是该字段为NULL
	FIRST_RETL_DATE DATE,
	-- 首次消费金额，无消费是该字段为NULL
	FIRST_RETL_AMT NUMBER(15,2) NOT NULL,
	TOT_DUE_AMT NUMBER(15,2) NOT NULL,
	CURR_DUE_AMT NUMBER(15,2) NOT NULL,
	-- 上个月最小还款额
	PAST_DUE_AMT1 NUMBER(15,2) NOT NULL,
	-- 上2个月最小还款额
	PAST_DUE_AMT2 NUMBER(15,2) NOT NULL,
	-- 上3个月最小还款额
	PAST_DUE_AMT3 NUMBER(15,2) NOT NULL,
	-- 上4个月最小还款额
	PAST_DUE_AMT4 NUMBER(15,2) NOT NULL,
	-- 上5个月最小还款额
	PAST_DUE_AMT5 NUMBER(15,2) NOT NULL,
	-- 上6个月最小还款额
	PAST_DUE_AMT6 NUMBER(15,2) NOT NULL,
	-- 上7个月最小还款额
	PAST_DUE_AMT7 NUMBER(15,2) NOT NULL,
	-- 上8个月最小还款额
	PAST_DUE_AMT8 NUMBER(15,2) NOT NULL,
	CTD_CASH_AMT NUMBER(15,2) NOT NULL,
	-- 本账期取现笔数
	CTD_CASH_CNT NUMBER NOT NULL,
	-- 本账期联机借记金额累计值，在授权时维护，批量不参与
	CTD_RETAIL_AMT NUMBER(15,2) NOT NULL,
	-- 本账期消费笔数
	CTD_RETAIL_CNT NUMBER NOT NULL,
	-- 本账期还款金额
	CTD_REPAY_AMT NUMBER(15,2) NOT NULL,
	-- 本账期还款笔数
	CTD_REPAY_CNT NUMBER NOT NULL,
	-- 本账期借记调整金额
	CTD_DB_ADJ_AMT NUMBER(15,2) NOT NULL,
	-- 本账期借记调整笔数
	CTD_DB_ADJ_CNT NUMBER NOT NULL,
	-- 本账期贷记调整金额
	CTD_CR_ADJ_AMT NUMBER(15,2) NOT NULL,
	-- 本账期贷记调整笔数
	CTD_CR_ADJ_CNT NUMBER NOT NULL,
	-- 本账期费用金额
	CTD_FEE_AMT NUMBER(15,2) NOT NULL,
	-- 本账期费用笔数
	CTD_FEE_CNT NUMBER NOT NULL,
	CTD_INTEREST_AMT NUMBER(15,2),
	CTD_INTEREST_CNT NUMBER,
	-- 本账期退货金额
	CTD_REFUND_AMT NUMBER(15,2) NOT NULL,
	-- 本账期退货笔数
	CTD_REFUND_CNT NUMBER NOT NULL,
	CTD_MAX_OVRLMT_AMT NUMBER(15,2) NOT NULL,
	MTD_RETAIL_AMT NUMBER(15,2) NOT NULL,
	-- 本月消费笔数
	MTD_RETAIL_CNT NUMBER NOT NULL,
	MTD_CASH_AMT NUMBER(15,2) NOT NULL,
	-- 本月取现笔数
	MTD_CASH_CNT NUMBER NOT NULL,
	MTD_REFUND_AMT NUMBER(15,2) NOT NULL,
	-- 本月退货笔数
	MTD_REFUND_CNT NUMBER NOT NULL,
	YTD_RETAIL_AMT NUMBER(15,2) NOT NULL,
	-- 本年消费笔数
	YTD_RETAIL_CNT NUMBER NOT NULL,
	-- 本年取现金额
	YTD_CASH_AMT NUMBER(15,2) NOT NULL,
	-- 本年取现次数
	YTD_CASH_CNT NUMBER NOT NULL,
	-- 本年退货金额
	YTD_REFUND_AMT NUMBER(15,2) NOT NULL,
	-- 本年退货次数
	YTD_REFUND_CNT NUMBER NOT NULL,
	YTD_OVRLMT_FEE_AMT NUMBER(15,2) NOT NULL,
	YTD_OVRLMT_FEE_CNT NUMBER NOT NULL,
	YTD_LATE_FEE_AMT NUMBER(15,2) NOT NULL,
	YTD_LATE_FEE_CNT NUMBER NOT NULL,
	LTD_RETAIL_AMT NUMBER(15,2) NOT NULL,
	-- 历史消费次数
	LTD_RETAIL_CNT NUMBER NOT NULL,
	LTD_CASH_AMT NUMBER(15,2) NOT NULL,
	LTD_CASH_CNT NUMBER NOT NULL,
	-- 历史退货金额
	LTD_REFUND_AMT NUMBER(15,2) NOT NULL,
	-- 历史退货次数
	LTD_REFUND_CNT NUMBER NOT NULL,
	-- 历史最高本金欠款
	LTD_HIGHEST_PRIN NUMBER(15,2) NOT NULL,
	-- 历史最高溢缴款
	LTD_HIGHEST_CR_BAL NUMBER(15,2) NOT NULL,
	-- 历史最高欠款
	LTD_HIGHEST_BAL NUMBER(15,2) NOT NULL,
	-- 进入催收队列次数
	COLLECT_CNT NUMBER,
	-- 催收系统分配的催收员代码
	COLLECTOR VARCHAR2(8),
	-- B0|锁定码催收，优先级第1
	-- A0|首次还款拖欠催收，优先级第2
	-- A1|账龄拖欠催收，优先级第3
	-- O0|超限催收，优先级第4
	-- ///
	-- @com.sunline.ppy.dictionary.enums.CollectionReason
	COLLECT_REASON CHAR(2),
	-- 过去24个账单周期的账龄，取值范围为0-9
	AGE_HST VARCHAR2(24),
	-- ‎记录过去24个月的还款历史，每个字节的含义如下：
	-- 引用枚举值com.sunline.ppy.dictionary.enums.PaymentStatus
	-- B|全额还款
	-- D|部分还款
	-- U|还款未达最小还款额
	-- N|未还款
	-- C|无需还款
	PAYMENT_HST VARCHAR2(24),
	-- ///
	-- @com.sunline.ppy.dictionary.enums.Indicator
	-- 
	WAIVE_OVLFEE_IND CHAR(1) NOT NULL,
	-- 账户上的卡年费免除标志对其下所有卡生，卡片上的仅对本卡生效
	-- ///
	-- @com.sunline.ppy.dictionary.enums.Indicator
	WAIVE_CARDFEE_IND CHAR(1) NOT NULL,
	-- ///
	-- @com.sunline.ppy.dictionary.enums.Indicator
	WAIVE_LATEFEE_IND CHAR(1) NOT NULL,
	-- ///
	-- @com.sunline.ppy.dictionary.enums.Indicator
	WAIVE_SVCFEE_IND CHAR(1) NOT NULL,
	USER_CODE1 VARCHAR2(4),
	USER_CODE2 VARCHAR2(4),
	USER_CODE3 VARCHAR2(4),
	USER_CODE4 VARCHAR2(4),
	USER_CODE5 VARCHAR2(4),
	USER_CODE6 VARCHAR2(4),
	USER_DATE1 DATE,
	USER_DATE2 DATE,
	USER_DATE3 DATE,
	USER_DATE4 DATE,
	USER_DATE5 DATE,
	USER_DATE6 DATE,
	USER_NUMBER1 NUMBER NOT NULL,
	USER_NUMBER2 NUMBER NOT NULL,
	USER_NUMBER3 NUMBER NOT NULL,
	USER_NUMBER4 NUMBER NOT NULL,
	USER_NUMBER5 NUMBER NOT NULL,
	USER_NUMBER6 NUMBER NOT NULL,
	USER_FIELD1 VARCHAR2(40),
	USER_FIELD2 VARCHAR2(40),
	USER_FIELD3 VARCHAR2(40),
	USER_FIELD4 VARCHAR2(40),
	USER_FIELD5 VARCHAR2(40),
	USER_FIELD6 VARCHAR2(40),
	USER_AMT1 NUMBER(15,2) NOT NULL,
	USER_AMT2 NUMBER(15,2) NOT NULL,
	USER_AMT3 NUMBER(15,2) NOT NULL,
	USER_AMT4 NUMBER(15,2) NOT NULL,
	USER_AMT5 NUMBER(15,2) NOT NULL,
	USER_AMT6 NUMBER(15,2) NOT NULL,
	JPA_VERSION NUMBER NOT NULL,
	MTD_PAYMENT_AMT NUMBER(15,2) NOT NULL,
	MTD_PAYMENT_CNT NUMBER NOT NULL,
	YTD_REPAY_AMT NUMBER(15,2) NOT NULL,
	YTD_REPAY_CNT NUMBER NOT NULL,
	LTD_REPAY_AMT NUMBER(15,2) NOT NULL,
	LTD_REPAY_CNT NUMBER NOT NULL,
	-- ///
	-- @com.sunline.ppy.dictionary.enums.SmsInd
	SMS_IND CHAR(1) NOT NULL,
	USER_SMS_AMT NUMBER(15,2),
	YTD_CYCLE_CHAG_CNT NUMBER,
	LAST_POST_DATE DATE,
	CONTR_NBR VARCHAR2(20),
	GUARANTY_ID VARCHAR2(32),
	DD_BANK_PROVINCE VARCHAR2(32),
	DD_BANK_PROVINCE_CODE VARCHAR2(10),
	DD_BANK_CITY VARCHAR2(32),
	DD_BANK_CITY_CODE VARCHAR2(10),
	PRIMARY KEY (ACCT_NBR, ACCT_TYPE)
);


CREATE TABLE CCS.CCS_ACCT_CRLMT_ADJ_LOG
(
	ORG CHAR(12) NOT NULL,
	-- 操作序列号
	OP_SEQ NUMBER(16) NOT NULL,
	OP_TIME TIMESTAMP NOT NULL,
	OP_ID VARCHAR2(40) NOT NULL,
	ACCT_NBR NUMBER(16) NOT NULL,
	-- ///
	-- @com.sunline.ppy.dictionary.enums.AccountType
	-- 
	ACCT_TYPE CHAR(1) NOT NULL,
	CARD_NBR VARCHAR2(19) NOT NULL,
	CREDIT_LMT_ORIG NUMBER(13) NOT NULL,
	CREDIT_LMT_NEW NUMBER(13) NOT NULL,
	PROC_DATE DATE NOT NULL,
	JPA_VERSION NUMBER NOT NULL,
	-- ///
	-- @com.sunline.ccs.param.def.enums.AdjState
	ADJ_STATE CHAR(1) NOT NULL,
	PRIMARY KEY (OP_SEQ)
);


-- 用于产生唯一账号
CREATE TABLE CCS.CCS_ACCT_NBR
(
	ORG CHAR(12) NOT NULL,
	ACCT_NBR NUMBER(16) NOT NULL,
	JPA_VERSION NUMBER NOT NULL,
	PRIMARY KEY (ACCT_NBR)
);


CREATE TABLE CCS.CCS_ACCT_O
(
	ORG CHAR(12) NOT NULL,
	ACCT_NBR NUMBER(16) NOT NULL,
	-- ///
	-- @com.sunline.ppy.dictionary.enums.AccountType
	-- 
	ACCT_TYPE CHAR(1) NOT NULL,
	CUST_LMT_ID NUMBER(16) NOT NULL,
	-- 顺序号，如未来需要校验位，可在后面补充
	CUST_ID NUMBER(16) NOT NULL,
	PRODUCT_CD CHAR(6) NOT NULL,
	CREDIT_LMT NUMBER(13) NOT NULL,
	TEMP_LMT NUMBER(13) NOT NULL,
	TEMP_LMT_BEG_DATE DATE,
	TEMP_LMT_END_DATE DATE,
	-- 授权允许的超限比例，来自账户下最高级别卡产品的参数定义。
	OVRLMT_RATE NUMBER(5,2),
	-- 取现额度比例（占用账户信用额度）
	CASH_LMT_RATE NUMBER(5,2),
	-- 额度内分期额度比例
	LOAN_LMT_RATE NUMBER(5,2),
	-- 当前欠款（负值表示有溢缴款）
	CURR_BAL NUMBER(15,2) NOT NULL,
	CASH_BAL NUMBER(15,2) NOT NULL,
	-- 当前额度内分期余额
	LOAN_BAL NUMBER(15,2) NOT NULL,
	-- 当前争议金额
	DISPUTE_AMT NUMBER(15,2) NOT NULL,
	BLOCK_CODE VARCHAR2(27),
	MEMO_CASH NUMBER(15,2) NOT NULL,
	MEMO_CR NUMBER(15,2) NOT NULL,
	MEMO_DB NUMBER(15,2) NOT NULL,
	LAST_MATCHED_OVERNIGHT_DATE DATE,
	-- 上次联机更新业务日期
	LAST_UPDATE_BIZ_DATE DATE,
	OWNING_BRANCH VARCHAR2(9) NOT NULL,
	-- 账户的账单日期
	CYCLE_DAY CHAR(2) NOT NULL,
	JPA_VERSION NUMBER NOT NULL,
	-- ///
	-- @com.sunline.ppy.dictionary.enums.SmsInd
	SMS_IND CHAR(1) NOT NULL,
	USER_SMS_AMT NUMBER(15,2),
	FLOAT_RATE NUMBER(5,2),
	CONTR_NBR VARCHAR2(20),
	GUARANTY_ID VARCHAR2(32),
	PRIMARY KEY (ACCT_NBR, ACCT_TYPE)
);


CREATE TABLE CCS.CCS_ADDRESS
(
	ORG CHAR(12) NOT NULL,
	ADDR_ID NUMBER(16) NOT NULL,
	-- 顺序号，无校验位
	CUST_ID NUMBER(16) NOT NULL,
	-- ///
	-- @com.sunline.ppy.dictionary.enums.AddressType
	ADDR_TYPE CHAR(1) NOT NULL,
	COUNTRY_CODE CHAR(3) NOT NULL,
	STATE VARCHAR2(80),
	CITY VARCHAR2(80),
	DISTRICT VARCHAR2(40),
	POSTCODE VARCHAR2(10),
	PHONE VARCHAR2(20) NOT NULL,
	ADDRESS VARCHAR2(200) NOT NULL,
	JPA_VERSION NUMBER NOT NULL,
	PRIMARY KEY (ADDR_ID)
);


CREATE TABLE CCS.CCS_AUTHMEMO_DEL_TMP
(
	ORG CHAR(12) NOT NULL,
	LOG_KV NUMBER(16) NOT NULL,
	JPA_VERSION NUMBER NOT NULL,
	ACCT_NBR NUMBER(16),
	-- 主卡逻辑卡号
	LOGIC_CARD_NBR VARCHAR2(19),
	-- ///
	-- @com.sunline.ppy.dictionary.enums.AuthTransType
	-- 
	TXN_TYPE VARCHAR2(20),
	CHB_TXN_AMT NUMBER(15,2),
	LOG_OL_TIME TIMESTAMP,
	LOG_BIZ_DATE DATE,
	-- ///
	-- @com.sunline.ppy.dictionary.enums.UnmatchStatus
	MEMO_STATUS CHAR(1),
	PRIMARY KEY (LOG_KV)
);


