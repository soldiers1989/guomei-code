	CORP_NAME VARCHAR2(80),
	CORP_TELEPH_NBR VARCHAR2(20),
	CORP_FAX VARCHAR2(20),
	-- ///
	-- @com.sunline.ppy.dictionary.enums.EmpPositionAttrType
	CORP_POSITION CHAR(1),
	-- A|个体户
	-- B|股份制
	-- C|政府部分
	-- D|私有企业
	-- E|跨国公司
	-- ///
	-- @com.sunline.ppy.dictionary.enums.CorpStructure
	CORP_STRUCTURE CHAR(1) NOT NULL,
	-- ///
	-- @com.sunline.ppy.dictionary.enums.TitleOfTechnicalType
	CORP_TECH_TITLE CHAR(1) NOT NULL,
	INCOME_PY NUMBER(12,2) NOT NULL,
	FAMILY_INCOME_PYP NUMBER(12,2) NOT NULL,
	-- ///
	-- @com.sunline.ppy.dictionary.enums.EmpType
	CORP_INDUSTRY_CATEGORY CHAR(1) NOT NULL,
	JPA_VERSION NUMBER NOT NULL,
	PRIMARY KEY (EMPLOYEE_ID)
);


