CREATE TABLE CCS.CCS_CARD
(
	ORG CHAR(12) NOT NULL,
	-- 主卡逻辑卡号
	LOGIC_CARD_NBR VARCHAR2(19) NOT NULL,
	ACCT_NBR NUMBER(16) NOT NULL,
	-- 顺序号，如未来需要校验位，可在后面补充
	CUST_ID NUMBER(16) NOT NULL,
	PRODUCT_CD CHAR(6) NOT NULL,
	APPL_NBR VARCHAR2(20) NOT NULL,
	BARCODE VARCHAR2(40),
	-- B|主卡
	-- S|副卡
	-- ///
	-- @com.sunline.ppy.dictionary.enums.BscSuppIndicator
	BSC_SUPP_IND CHAR(1) NOT NULL,
	-- 逻辑卡主卡卡号
	CARD_BASIC_NBR VARCHAR2(19),
	OWNING_BRANCH VARCHAR2(9) NOT NULL,
	APPL_PROMO_CODE VARCHAR2(15),
	RECOM_NAME VARCHAR2(80),
	RECOM_CARD_NO VARCHAR2(19),
	SETUP_DATE DATE NOT NULL,
	BLOCK_CODE VARCHAR2(27),
	-- ///
	-- @com.sunline.ppy.dictionary.enums.Indicator
	ACTIVE_IND CHAR(1) NOT NULL,
	ACTIVE_DATE DATE,
	-- 卡片账户置上待销卡销户锁定码的日期
	-- 在销卡时置上销卡日期，在销卡撤销时清零销卡日期
	CLOSE_DATE DATE,
	-- 最新物理卡号
	LASTEST_MEDIUM_CARD_NBR VARCHAR2(19) NOT NULL,
	-- ///
	-- @com.sunline.ppy.dictionary.enums.Indicator
	SALES_IND CHAR(1) NOT NULL,
	APPL_SRC VARCHAR2(20),
	REPRESENT_NAME VARCHAR2(80),
	-- ///
	-- @com.sunline.ppy.dictionary.enums.Indicator
	POS_PIN_VERIFY_IND CHAR(1) NOT NULL,
	-- C|配偶
	-- F|父亲
	-- M|母亲
	-- B|兄弟
	-- S|姐妹
	-- L|亲属
	-- W|同事
	-- ///
	-- @com.sunline.ppy.dictionary.enums.Relationship
	-- 
	RELATIONSHIP_TO_BSC CHAR(1),
	CARD_EXPIRE_DATE DATE NOT NULL,
	CARD_FEE_RATE NUMBER(5,2) NOT NULL,
	-- ///
	-- @com.sunline.ppy.dictionary.enums.RenewInd
	-- 
	RENEW_IND CHAR(1) NOT NULL,
	-- ///
	-- @com.sunline.ppy.dictionary.enums.RenewResult
	RENEW_REJECT_CD CHAR(3),
	FIRST_CARD_FEE_DATE DATE,
	-- 在做到期换卡时更新该字段
	LAST_RENEWCARD_DATE DATE,
	-- A|表示未用卡
	-- B|表示首次用卡
	-- C|表示首次用卡且收过年费
	-- ///
	-- @com.sunline.ccs.param.def.enums.FirstUsageIndicator
	FIRST_USAGE_FLAG CHAR(1) NOT NULL,
	NEXT_CARD_FEE_DATE DATE,
	-- 账户上的卡年费免除标志对其下所有卡生，卡片上的仅对本卡生效
	-- ///
	-- @com.sunline.ppy.dictionary.enums.Indicator
	WAIVE_CARDFEE_IND CHAR(1) NOT NULL,
	-- A|将卡片寄送给持卡人
	-- B|分行领卡
	-- ///
	-- @com.sunline.ppy.dictionary.enums.CardFetchMethod
	CARD_DELIVER_METHOD CHAR(1) NOT NULL,
	-- ///
	-- @com.sunline.ppy.dictionary.enums.AddressType
	CARD_DELIVER_ADDR_FLAG CHAR(1),
	JPA_VERSION NUMBER NOT NULL,
	FIRST_USAGE_DATE DATE,
	PRIMARY KEY (LOGIC_CARD_NBR)
);


