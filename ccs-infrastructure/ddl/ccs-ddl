CREATE TABLE CCS.CCS_CUSTOMER
(
	ORG CHAR(12) NOT NULL,
	-- 顺序号，如未来需要校验位，可在后面补充
	CUST_ID NUMBER(16) NOT NULL,
	ID_NO VARCHAR2(30) NOT NULL,
	-- ///
	-- @com.sunline.ppy.dictionary.enums.IdType
	ID_TYPE VARCHAR2(1) NOT NULL,
	-- 可以为Mr./Mrs/Madam等
	-- ///
	-- @com.sunline.ppy.dictionary.enums.EmpPositionAttrType
	TITLE CHAR(1),
	-- 客户姓名 - 冗余字段，从客户表复制过来，这样批量处理时可以不用关联客户信息
	NAME VARCHAR2(80) NOT NULL,
	-- M|男
	-- F|女
	-- ///
	-- @com.sunline.ppy.dictionary.enums.Gender
	GENDER CHAR(1) NOT NULL,
	BIRTHDAY DATE NOT NULL,
	-- ///
	-- @com.sunline.ppy.dictionary.enums.OccupationType
	OCCUPATION CHAR(1),
	INTERNAL_STAFF_ID VARCHAR2(20),
	NATIONALITY CHAR(3),
	-- ///
	-- @com.sunline.ppy.dictionary.enums.Indicator
	PR_OF_COUNTRY CHAR(1),
	RESIDENCY_COUNTRY_CD CHAR(3),
	-- M|已婚
	-- S|未婚
	-- W|丧偶
	-- D|离异
	-- ///
	-- @com.sunline.ppy.dictionary.enums.MaritalStatus
	-- 
	MARITAL_STATUS CHAR(1),
	-- ///
	-- @com.sunline.ppy.dictionary.enums.EducationType
	EDUCATION CHAR(1),
	-- N|普通
	-- F|高风险
	-- ///
	-- @com.sunline.ppy.dictionary.enums.SocialStatus
	SOCIAL_STATUS CHAR(1),
	ID_ISS_ADDR VARCHAR2(200),
	HOME_PHONE VARCHAR2(20),
	-- ///
	-- @com.sunline.ppy.dictionary.enums.HouseOwnership
	HOUSE_OWNERSHIP CHAR(1),
	-- ///
	-- @com.sunline.ppy.dictionary.enums.HouseType
	HOUSE_TYPE CHAR(1),
	-- 现住址居住起始年月
	HOME_STAND_FROM DATE,
	-- A|少于50000
	-- B|50K-100K
	-- C|100K-500K
	-- D|500K-900K
	-- E|大于900K
	-- ///
	-- @com.sunline.ppy.dictionary.enums.LiquidAsset
	LIQUID_ASSET CHAR(1),
	MOBILE_NO VARCHAR2(20) NOT NULL,
	EMAIL VARCHAR2(80),
	-- ///
	-- @com.sunline.ppy.dictionary.enums.Indicator
	EMP_STATUS CHAR(1),
	NBR_OF_DEPENDENTS NUMBER,
	LANGUAGE_IND VARCHAR2(4),
	SETUP_DATE DATE NOT NULL,
	SOCIAL_INS_AMT NUMBER(15,2) NOT NULL,
	DRIVE_LICENSE_ID VARCHAR2(20),
	DRIVE_LIC_REG_DATE DATE,
	-- 预留问题
	SECURE_QUESTION VARCHAR2(80),
	-- 预留答案
	SECURE_ANSWER VARCHAR2(80),
	-- ///
	-- @com.sunline.ppy.dictionary.enums.EmpStability
	EMP_QUIT_FREQ CHAR(1),
	CORP_NAME VARCHAR2(80),
	USER_CODE1 VARCHAR2(4),
	USER_CODE2 VARCHAR2(4),
	USER_CODE3 VARCHAR2(4),
	USER_CODE4 VARCHAR2(4),
	USER_CODE5 VARCHAR2(4),
	USER_CODE6 VARCHAR2(4),
	USER_DATE1 DATE,
	USER_DATE2 DATE,
	USER_DATE3 DATE,
	USER_DATE4 DATE,
	USER_DATE5 DATE,
	USER_DATE6 DATE,
	USER_NUMBER1 NUMBER NOT NULL,
	USER_NUMBER2 NUMBER NOT NULL,
	USER_NUMBER3 NUMBER NOT NULL,
	USER_NUMBER4 NUMBER NOT NULL,
	USER_NUMBER5 NUMBER NOT NULL,
	USER_NUMBER6 NUMBER NOT NULL,
	USER_FIELD1 VARCHAR2(40),
	USER_FIELD2 VARCHAR2(40),
	USER_FIELD3 VARCHAR2(40),
	USER_FIELD4 VARCHAR2(40),
	USER_FIELD5 VARCHAR2(40),
	USER_FIELD6 VARCHAR2(40),
	USER_AMT1 NUMBER(15,2) NOT NULL,
	USER_AMT2 NUMBER(15,2) NOT NULL,
	USER_AMT3 NUMBER(15,2) NOT NULL,
	USER_AMT4 NUMBER(15,2) NOT NULL,
	USER_AMT5 NUMBER(15,2) NOT NULL,
	USER_AMT6 NUMBER(15,2) NOT NULL,
	INTERNAL_CUSTOMER_ID VARCHAR2(32),
	ONCARD_NAME VARCHAR2(26) NOT NULL,
	JPA_VERSION NUMBER NOT NULL,
	CUST_LMT_ID NUMBER(16),
	PRIMARY KEY (CUST_ID)
);


CREATE TABLE CCS.CCS_CUSTOMER_CRLMT
(
	ORG CHAR(12) NOT NULL,
	CUST_LMT_ID NUMBER(16) NOT NULL,
	-- ///
	-- @com.sunline.ppy.dictionary.enums.LimitCategory
	LMT_CATEGROY VARCHAR2(20) NOT NULL,
	-- S|SUM，客户额度取其下账户额度之和
	-- H|HIGHEST，客户额度去其下账户额度最大值
	-- ///
	-- @com.sunline.ccs.param.def.enums.LimitType
	-- 
	LMT_CALC_METHOD CHAR(1) NOT NULL,
	CREDIT_LMT NUMBER(13) NOT NULL,
	JPA_VERSION NUMBER NOT NULL,
	PRIMARY KEY (CUST_LMT_ID)
);


-- 工作单位信息
CREATE TABLE CCS.CCS_EMPLOYEE
(
	ORG CHAR(12) NOT NULL,
	EMPLOYEE_ID NUMBER(16) NOT NULL,
	-- 顺序号，如未来需要校验位，可在后面补充
