);


CREATE TABLE CCS.CCS_CARD_O
(
	ORG CHAR(12) NOT NULL,
	-- 主卡逻辑卡号
	LOGIC_CARD_NBR VARCHAR2(19) NOT NULL,
	-- 顺序号，如未来需要校验位，可在后面补充
	CUST_ID NUMBER(16) NOT NULL,
	CUST_LMT_ID NUMBER(16) NOT NULL,
	ACCT_NBR NUMBER(16) NOT NULL,
	PRODUCT_CD CHAR(6) NOT NULL,
	-- B|主卡
	-- S|副卡
	-- ///
	-- @com.sunline.ppy.dictionary.enums.BscSuppIndicator
	BSC_SUPP_IND CHAR(1) NOT NULL,
	-- 逻辑卡主卡卡号
	CARD_BASIC_NBR VARCHAR2(19),
	-- ///
	-- @com.sunline.ppy.dictionary.enums.Indicator
	ACTIVE_IND CHAR(1) NOT NULL,
	-- ///
	-- @com.sunline.ppy.dictionary.enums.Indicator
	POS_PIN_VERIFY_IND CHAR(1) NOT NULL,
	-- 交易密码错误次数
	PIN_TRIES NUMBER NOT NULL,
	BLOCK_CODE VARCHAR2(27),
	-- 全9 - 无限额；新建卡若外部未送入，则赋值为全9
	CYCLE_RETAIL_LMT NUMBER(13) NOT NULL,
	-- 全9 - 无限额；新建卡若外部未送入，则赋值为全9
	CYCLE_CASH_LMT NUMBER(13) NOT NULL,
	-- 全9 - 无限额；新建卡若外部未送入，则赋值为全9
	CYCLE_NET_LMT NUMBER(13) NOT NULL,
	-- 全9 - 无限额；新建卡若外部未送入，则赋值为全9
	TXN_LMT NUMBER(13) NOT NULL,
	-- 全9 - 无限额；新建卡若外部未送入，则赋值为全9
	TXN_CASH_LMT NUMBER(13) NOT NULL,
	-- 全9 - 无限额；新建卡若外部未送入，则赋值为全9
	TXN_NET_LMT NUMBER(13) NOT NULL,
	-- 当天ATM联机取现交易笔数累计值
	DAY_USED_ATM_NBR NUMBER NOT NULL,
	-- 当天ATM渠道联机取现金额累计值
	DAY_USED_ATM_AMT NUMBER(15,2) NOT NULL,
	DAY_USED_RETAIL_NBR NUMBER NOT NULL,
	DAY_USED_RETAIL_AMT NUMBER(15,2) NOT NULL,
	DAY_USED_CASH_NBR NUMBER NOT NULL,
	DAY_USED_CASH_AMT NUMBER(15,2) NOT NULL,
	DAY_USED_XFROUT_NBR NUMBER NOT NULL,
	DAY_USED_XFROUT_AMT NUMBER(15,2) NOT NULL,
	-- 本账期联机消费金额累计值（包含 消费、预授权等），在授权时维护，批量不参与 
	CTD_USED_AMT NUMBER(15,2) NOT NULL,
	CTD_CASH_AMT NUMBER(15,2) NOT NULL,
	-- 本账期网银消费金额累计值，在授权时维护，批量不参与
	CTD_NET_AMT NUMBER(15,2) NOT NULL,
	-- 上次联机更新业务日期
	LAST_UPDATE_BIZ_DATE DATE,
	JPA_VERSION NUMBER NOT NULL,
	LAST_PIN_TRIES_TIME TIMESTAMP,
	INQ_PIN_TRIES NUMBER NOT NULL,
	LAST_INQ_PIN_TRIES_TIME TIMESTAMP,
	DAY_USED_ATM_CUPXB_AMT NUMBER(15,2) NOT NULL,
	PRIMARY KEY (LOGIC_CARD_NBR)
);


