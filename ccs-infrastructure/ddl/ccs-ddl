/* Create Tables */

-- 卡号生成信息表
CREATE TABLE CCS.CCS_CARDNBR_GRT
(
	ORG CHAR(12) NOT NULL,
	PRODUCT_CD CHAR(6) NOT NULL,
	-- 当前值
	CURR_VALUE NUMBER(19),
	PRIMARY KEY (ORG, PRODUCT_CD)
);


CREATE TABLE CCS.CCS_ORDER
(
	ORG CHAR(12),
	ORDER_ID NUMBER(16) NOT NULL,
	CHANNEL_ID VARCHAR2(10),
	LOG_KV NUMBER(16),
	ACCT_NBR NUMBER(16),
	-- ///
	-- @com.sunline.ppy.dictionary.enums.AccountType
	-- 
	ACCT_TYPE CHAR(1),
	-- ///
	-- @com.sunline.ppy.dictionary.enums.CommandType
	COMMAND_TYPE CHAR(3),
	ORDER_BRIEF VARCHAR2(400),
	-- ///
	-- @com.sunline.ppy.dictionary.enums.OrderStatus
	ORDER_STATUS CHAR(1),
	ORDER_TIME TIMESTAMP,
	ORBER_FAIL_TIME TIMESTAMP,
	MER_ID VARCHAR2(30),
	MER_NAME VARCHAR2(80),
	PRODUCT_TYPE VARCHAR2(10),
	-- ///
	-- @com.sunline.ppy.dictionary.enums.AuthTransType
	-- 
	TXN_TYPE VARCHAR2(20),
	TXN_AMT NUMBER(15,2),
	CURRENCY CHAR(3),
	OPEN_BANK_ID VARCHAR2(10),
	OPEN_BANK VARCHAR2(100),
	-- 0表示卡,1表示折
	CARD_TYPE CHAR(1),
	CARD_NO VARCHAR2(32),
	USR_NAME VARCHAR2(100),
	-- 身份证01，军官证02 
	-- 护照03，户口簿04 
	-- 回乡证05 
	-- 其他06
	CERT_TYPE VARCHAR2(2),
	CERT_ID VARCHAR2(25),
	PURPOSE VARCHAR2(25),
	PRIV1 VARCHAR2(120),
	STATE VARCHAR2(80),
	CITY VARCHAR2(80),
	SUB_BANK VARCHAR2(200),
	-- “00”对私，“01”对公
	FLAG VARCHAR2(2),
	GUARANTY_ID VARCHAR2(32),
	-- 支付状态为SUCCESS成功，其他为失败
	STATUS VARCHAR2(8),
	CODE VARCHAR2(8),
	MESSAGE VARCHAR2(64),
	DUE_BILL_NO VARCHAR2(30),
	BUSINESS_DATE DATE,
	SEND_TIME DATE,
	ORI_ORDER_ID NUMBER,
	JPA_VERSION NUMBER,
	SETUP_DATE DATE,
	OPT_DATETIME TIMESTAMP,
	-- ///
	-- @com.sunline.ppy.dictionary.enums.LoanUsage
	LOAN_USAGE VARCHAR2(2),
	-- 主业务：0长亮核算：1
	PAY_CHANNEL_ID VARCHAR2(3),
	-- 实时或批量代扣：0
	-- 实时或批量代付：1
	PAY_BIZ_CODE VARCHAR2(4),
	PRIMARY KEY (ORDER_ID)
);


CREATE TABLE CCS.CCS_ORDER_HST
(
	ORG CHAR(12),
	ORDER_ID NUMBER(16) NOT NULL,
	CHANNEL_ID VARCHAR2(10),
	LOG_KV NUMBER(16),
	ACCT_NBR NUMBER(16),
	-- ///
	-- @com.sunline.ppy.dictionary.enums.AccountType
	-- 
	ACCT_TYPE CHAR(1),
	-- ///
	-- @com.sunline.ppy.dictionary.enums.CommandType
	COMMAND_TYPE CHAR(3),
	ORDER_BRIEF VARCHAR2(400),
	-- ///
	-- @com.sunline.ppy.dictionary.enums.OrderStatus
	ORDER_STATUS CHAR(1),
	ORDER_TIME TIMESTAMP,
	ORBER_FAIL_TIME TIMESTAMP,
	MER_ID VARCHAR2(30),
	MER_NAME VARCHAR2(80),
	PRODUCT_TYPE VARCHAR2(10),
	-- ///
	-- @com.sunline.ppy.dictionary.enums.AuthTransType
	-- 
	TXN_TYPE VARCHAR2(20),
	TXN_AMT NUMBER(15,2),
	CURRENCY CHAR(3),
	OPEN_BANK_ID VARCHAR2(10),
	OPEN_BANK VARCHAR2(100),
	-- 0表示卡,1表示折
	CARD_TYPE CHAR(1),
	CARD_NO VARCHAR2(32),
	USR_NAME VARCHAR2(100),
	-- 身份证01，军官证02 
	-- 护照03，户口簿04 
	-- 回乡证05 
	-- 其他06
	CERT_TYPE VARCHAR2(2),
	CERT_ID VARCHAR2(25),
	PURPOSE VARCHAR2(25),
	PRIV1 VARCHAR2(120),
	STATE VARCHAR2(80),
	CITY VARCHAR2(80),
	SUB_BANK VARCHAR2(200),
	-- “00”对私，“01”对公
	FLAG VARCHAR2(2),
	GUARANTY_ID VARCHAR2(32),
	-- 支付状态为SUCCESS成功，其他为失败
	STATUS VARCHAR2(8),
	CODE VARCHAR2(8),
	MESSAGE VARCHAR2(64),
	DUE_BILL_NO VARCHAR2(30),
	BUSINESS_DATE DATE,
	SEND_TIME DATE,
	ORI_ORDER_ID NUMBER,
	JPA_VERSION NUMBER,
	SETUP_DATE DATE,
	OPT_DATETIME TIMESTAMP,
	-- ///
	-- @com.sunline.ppy.dictionary.enums.LoanUsage
	LOAN_USAGE VARCHAR2(2),
	-- 实时或批量代扣：0
	-- 实时或批量代付：1
	PAY_BIZ_CODE VARCHAR2(4),
	-- 主业务：0长亮核算：1
	PAY_CHANNEL_ID VARCHAR2(3),
	PRIMARY KEY (ORDER_ID)
);


CREATE TABLE CCS.CCS_SETTLE_CLAIM
(
	ORG CHAR(12),
	LOAN_ID NUMBER(16) NOT NULL,
	ACCT_NBR NUMBER(16),
	CARD_NBR VARCHAR2(19),
	CONTR_NBR VARCHAR2(20),
	DUE_BILL_NO VARCHAR2(30),
	GUARANTY_ID VARCHAR2(32),
	REGISTER_DATE DATE,
	-- R|消费转分期
	-- C|现金转分期
	-- B|账单分期
	-- P|POS分期
	-- M|大额分期（专项分期）
	-- ///
	-- @com.sunline.ppy.dictionary.enums.LoanType
	LOAN_TYPE VARCHAR2(5),
	-- ///
	-- @com.sunline.ppy.dictionary.enums.LoanStatus
	LOAN_STATUS CHAR(1),
	LOAN_INIT_TERM NUMBER,
	REMAIN_TERM NUMBER,
	LOAN_INIT_PRIN NUMBER(15,2),
	LOAN_INIT_FEE NUMBER(15,2),
	PAID_PRINCIPAL NUMBER(15,2),
	PAID_INTEREST NUMBER(15,2),
	PAID_FEE NUMBER(15,2),
	COMPOUND_RATE NUMBER(12,4),
	INTEREST_RATE NUMBER(12,4),
	PENALTY_RATE NUMBER(12,4),
	OVERDUE_DATE DATE,
	-- 约定还款扣款账户银行名称
	DD_BANK_NAME VARCHAR2(160),
	DD_BANK_BRANCH VARCHAR2(9),
	-- 约定还款扣款账号
	DD_BANK_ACCT_NBR VARCHAR2(40),
	-- 约定还款扣款账户姓名
	DD_BANK_ACCT_NAME VARCHAR2(160),
	-- 当前欠款（负值表示有溢缴款）
	CURR_BAL NUMBER(15,2),
	-- 持卡人还清该欠款，则会对账户进行全额还款免息，该金额会显示在账单上，小于等于账户账单余额
	QUAL_GRACE_BAL NUMBER(15,2),
	SETTLE_DATE DATE,
	SETTLE_AMT NUMBER(15,2),
	SETTLE_PRINCIPAL NUMBER(15,2),
	COMPENSATORY_AMT NUMBER(15,2),
	LAST_COMPENSATORY_DATE DATE,
	JPA_VERSION NUMBER,
	INSURANCE_RATE NUMBER(12,4),
	SETTLE_INTEREST NUMBER(15,2),
	SETTLE_INSURANCE_AMT NUMBER(15,2),
	SETTLE_ADDEDVALUETAX_AMT NUMBER(15,2),
	SETTLE_STAMPDUTY_AMT NUMBER(15,2),
	-- Y|是
	-- N|否
	-- ///
	-- @com.sunline.ppy.dictionary.enums.Indicator
	SETTLE_FLAG CHAR(1),
	COMPENSATORY_FLAG CHAR(1),
	PRIMARY KEY (LOAN_ID)
);


-- 当日交易入账表
CREATE TABLE CCS.CCS_TXN_POST
(
	TXN_ID NUMBER(16) NOT NULL,
	MTI CHAR(4),
	ORG CHAR(12),
	TXN_SOURCE CHAR(2),
	-- ///
	-- @com.sunline.ppy.dictionary.enums.InputSource
	SRC_CHNL VARCHAR2(12),
	TXN_CODE CHAR(3),
	-- ///
	-- @com.sunline.ppy.dictionary.enums.AuthTransType
	-- 
	TXN_TYPE VARCHAR2(20),
	-- ///
	-- @com.sunline.ccs.param.def.enums.AuthTransDirection
	TXN_DIRECTION VARCHAR2(20),
	-- D|借记交易
	-- C|贷记交易
	-- M|memo交易
	-- ///
	-- @com.sunline.ppy.dictionary.enums.DbCrInd
	DB_CR_IND CHAR(1),
	CARD_NBR VARCHAR2(19),
	TXN_AMT NUMBER(15,2),
	TXN_CURRENCT_CODE CHAR(3),
	SETT_AMT NUMBER(15,2),
	SETT_CURRENCY_CODE CHAR(3),
	TXN_TIME CHAR(10),
	REF_NBR VARCHAR2(32),
	AUTH_CODE CHAR(6),
	ACQ_BRANCH_IQ VARCHAR2(9),
	ACQ_TERM_ID CHAR(8),
	ACQ_ACCEPTOR_ID VARCHAR2(15),
	ACQ_ADDRESS VARCHAR2(40),
	MERCH_CATEGORY_CODE CHAR(4),
	ORIG_TXN_INFO VARCHAR2(42),
	ORIG_TXN_TIME CHAR(10),
	ORIG_TRACE_NO CHAR(6),
	VOUCHER_NO VARCHAR2(7),
	TXN_FEE_AMT NUMBER(15,2),
	-- 发卡方应得手续费
	FEE_PROFIT NUMBER(15,2),
	B003_PROC_CODE CHAR(6),
	B011_TRACE CHAR(6),
	B022_ENTRYMODE VARCHAR2(3),
	B025_ENTRYCOND CHAR(2),
	B032_ACQ_INST VARCHAR2(11),
	B033_FWD_INS VARCHAR2(11),
	B039_RTN_CODE VARCHAR2(10),
	B060 VARCHAR2(30),
	B061 VARCHAR2(200),
	BATCH_DATE DATE,
	-- ///
	-- @com.sunline.ppy.dictionary.enums.Indicator
	COMPARED_IND CHAR(1),
	-- ///
	-- @com.sunline.ppy.dictionary.enums.Indicator
	IS_REVOKE CHAR(1),
	-- ///
	-- @com.sunline.ppy.dictionary.enums.Indicator
	EXP_IND CHAR(1),
	-- ///
	-- @com.sunline.ppy.dictionary.enums.Indicator
	ERR_IND CHAR(1),
	-- ///
	-- @com.sunline.ppy.dictionary.enums.Indicator
	MATCH_IND CHAR(1),
	B040_TERM_ID CHAR(3),
	CREATE_TIME TIMESTAMP DEFAULT SYSDATE,
	CREATE_USER VARCHAR2(32),
	LST_UPD_TIME TIMESTAMP DEFAULT SYSDATE,
	LST_UPD_USER VARCHAR2(32),
	JPA_VERSION NUMBER(10,0),
	PRIMARY KEY (TXN_ID)
);


-- 当日交易入账历史表
CREATE TABLE CCS.CCS_TXN_POST_HST
(
	TXN_ID NUMBER(16) NOT NULL,
	MTI CHAR(4),
	ORG CHAR(12),
	TXN_SOURCE CHAR(2),
	-- ///
